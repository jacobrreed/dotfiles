{
  "layer": "top",
  "output": [
    "DP-1",
    "DP-2",
    "DP-3"
  ],
  "height": 30,
  "margin": 8,
  "spacing": 4,
  "reload_style_on_change": true,
  "fixed-center": true,
  // MODULES
  "modules-left": [
    "custom/icon",
    "hyprland/workspaces",
    "hyprland/submap",
    "mpris"
  ],
  "modules-center": [
    "hyprland/window"
  ],
  "modules-right": [
    "group/system",
    "group/actions",
    "clock",
    "tray"
  ],
  // GROUPS
  "group/system": {
    "orientation": "horizontal",
    "modules": [
      "network",
      "custom/separator",
      "wireplumber",
      "custom/separator",
      "memory",
      "cpu"
    ]
  },
  "group/actions": {
    "orientation": "horizontal",
    "modules": [
      "custom/notification",
      "idle_inhibitor",
      "backlight",
      "battery"
    ]
  },
  // MODULE CONFIGURATIONS
  "hyprland/workspaces": {
    "on-click": "activate",
    "format": "{id} {windows}", // Only show workspace number
    "format-window-separator": " ",
    "workspace-taskbar": {
      "enable": true,
      "update-active-window": true,
      "format": "{icon}",
      "icon-size": 18
    }
  },
  "hyprland/window": {
    "format": "{class} - {title}",
    "max-length": 60,
    "rewrite": {
      "firefox - (.*)": "Ôâ©  $1", // Firefox
      "firefox-developer-edition - (.*)": "Ôâ©  $1", // Firefox Dev
      "thunderbird - (.*)": "Ôç∞  $1", // Firefox Dev
      "kitty - (.*)": "üò∫Ô∏è $1", // Kitty terminal
      "Steam - (.*)": "ÔÜ∂  $1", // Steam
      "discord - (.*)": "Ôéí  $1", // Discord
      "spotify - (.*)": "ÔÜº  $1" // Spotify
    },
    "separate-outputs": true
  },
  "hyprland/submap": {
    "max-length": 16,
    "format": "Û∞©® Resize Mode"
  },
  "idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
      "activated": "ÔÅÆ ",
      "deactivated": "ÔÅ∞ "
    }
    // Send notification TODO
    // "on-click": "$HOME/.config/waybar/scripts/toggle-idle.sh"
  },
  "wireplumber": {
    "format": "{icon} {volume}%",
    "format-muted": "ÔÄ¶ Muted",
    // "format-muted": " {format_source}",
    // "format-source": "{source_volume}% ",
    // "format-source-muted": " ",
    "format-icons": [
      "ÔÄ¶ ",
      "ÔÄß ",
      "ÔÄ® "
    ],
    "scroll-step": 0.2,
    "max-volume": 100,
    "on-click": "pavucontrol",
    "on-click-right": "qpwgraph",
    "on-scroll-up": "pactl set-sink-volume @DEFAULT_SINK@ +5%",
    "on-scroll-down": "pactl set-sink-volume @DEFAULT_SINK@ -5%"
  },
  "mpris": {
    "format": "{player_icon} {artist} - {title}",
    "format-paused": "{status_icon} {artist} - {title}",
    "max-length": 60,
    "player-icons": {
      "default": "Û∞éÜ ",
      "cmus": "ÔÄÅ",
      "mpv": "üéµ",
      "vlc": "üé¨",
      "spotify": "ÔÜº "
    },
    "status-icons": {
      "paused": "Û∞è¶"
    },
    "on-click": "playerctl play-pause",
    "on-click-right": "playerctl next",
    "smooth-scrolling-threshold": 10,
    "on-scroll-down": "playerctl previous"
    // "ignored-players": ["cmus", "spotify"]
  },
  "cava": {
    "framerate": 30,
    "autosens": 1,
    "sensitivity": 1000,
    "bars": 12,
    "lower_cutoff_freq": 10,
    "higher_cutoff_freq": 200,
    "hide_on_silence": false,
    "method": "pulse",
    "source": "auto",
    "stereo": true,
    "reverse": false,
    "bar_delimiter": 0,
    "monstercat": true,
    "waves": true,
    "noise_reduction": 0.77,
    "input_delay": 2,
    "format-icons": [
      "‚ñÅ",
      "‚ñÇ",
      "‚ñÉ",
      "‚ñÑ",
      "‚ñÖ",
      "‚ñÜ",
      "‚ñá",
      "‚ñà"
    ],
    "actions": {
      "on-click-right": "mode"
    }
  },
  "network": {
    "interface": "enp10s0",
    "format": "{ifname}",
    "format-wifi": "Ôá´  {ifname} {bandwidthDownBytes}‚Üì {bandwidthUpBytes}‚Üë",
    "format-ethernet": "Û∞àÄ {bandwidthDownBytes}‚Üì {bandwidthUpBytes}‚Üë",
    "format-disconnected": "Û∞§Æ ", //An empty format will hide the module.
    "tooltip-format": "{ifname} via {gwaddr} Û∞äó ",
    "tooltip-format-wifi": "{essid} ({signalStrength}%) Ôá´ ",
    "tooltip-format-ethernet": "{ifname} ÔÉÅ ",
    "tooltip-format-disconnected": "Disconnected",
    "max-length": 50,
    "on-click": "nm-connection-editor"
  },
  "battery": {
    "states": {
      "medium": 40,
      "warning": 30,
      "critical": 15
    },
    "format": "{capacity}% {icon}",
    "format-icons": [
      "ÔâÑ ",
      "ÔâÉ ",
      "ÔâÇ ",
      "ÔâÅ ",
      "ÔâÄ "
    ],
    "format-charging": "Ôá¶ {capacity}% {icon}",
    "interval": 5
  },
  "clock": {
    "format": "{:%d %b %y %H:%M:%S}",
    "interval": 1,
    "tooltip": true,
    "tooltip-format": "{:%A, %B %d, %Y}"
  },
  "backlight": {
    "device": "intel_backlight",
    "format": "{percent}% Û∞õ® "
  },
  "tray": {
    "icon-size": 21,
    "spacing": 10
  },
  "cpu": {
    "interval": 10,
    "format": "{usage}% Ôãõ ",
    "max-length": 10
  },
  "memory": {
    "interval": 10,
    "format": "{percentage}% ÔÉâ  ",
    "max-length": 10
  },
  // CUSTOMS
  "custom/icon": {
    "format": "Û∞£á",
    "on-click": "wlogout --protocol layer-shell -b 3 -T 400 -B 400 -L 400 -R 400"
  },
  "custom/notification": {
    "format": "ÔÉ≥  "
    // "on-click-right": "dunstctl set-paused toggle"
  },
  "custom/separator": {
    "format": " ‚îÜ "
  }
}
